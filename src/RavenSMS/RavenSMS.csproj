		<ImplicitUsings>true</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup>
		<Title>RavenSMS</Title>
		<PackageId>RavenSMS</PackageId>
		<PackageTags>RavenSMS; SMS;</PackageTags>
